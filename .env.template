# Chat model for filtering
AZURE_OPENAI_CHAT_DEPLOYMENT=

# Local CSV path
CSV_PATH=c:/Source/AI-Policy-Agent/PolicyDocTitles.csv

# Optional: If you plan to use an indexer via Blob Storage
AZURE_STORAGE_CONNECTION_STRING=
AZURE_STORAGE_CONTAINER=policies
BLOB_NAME=PolicyDocTitles.csv
